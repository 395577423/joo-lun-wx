/* 加减框 */
.num-box{width: 150rpx;
  height: 50rpx;
  border: 1rpx solid #ebeef5;
  border-radius: 8rpx;
  display: flex;
  box-sizing: border-box;
}
.num-box .minus-num{
  width: 50rpx;
  height: 100%;
  border-right: 1rpx solid #ebeef5;
  display: flex;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
}
.num-box .number{
   width: 50rpx;
  height: 100%;
  border-right: 1rpx solid #ebeef5;
  box-sizing: border-box;
}
.num-box .number .input{
  width: 50rpx;
  height: 100%;
  text-align: center;
  font-size: 26rpx;
  color: #333;
}
.num-box .plus-num{
  width: 50rpx;
  height: 50rpx;
    display: flex;
  justify-content: center;
  align-items: center;
}
.bg-size{background-size: 25rpx 25rpx;background-repeat: no-repeat;background-position: center center;}
